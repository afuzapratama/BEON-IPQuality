# Environment Variables for BEON-IPQuality
# Copy this file to .env and modify values

# PostgreSQL
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_USER=beon
POSTGRES_DB=beon_ipquality

# ClickHouse
CLICKHOUSE_PASSWORD=your_clickhouse_password

# Redis (if password required)
REDIS_PASSWORD=

# Grafana
GRAFANA_PASSWORD=your_grafana_password

# API Keys
API_MASTER_KEY=your_master_api_key_here

# MaxMind License Key (for GeoLite2 downloads)
MAXMIND_LICENSE_KEY=your_maxmind_license_key

# Environment
ENVIRONMENT=production
LOG_LEVEL=info
